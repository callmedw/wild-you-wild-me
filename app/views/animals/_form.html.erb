<%= render "layouts/errors", :object => @animal %>

<%= form_for @animal do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :species %>
  <%= f.text_field :species %>
  <%= f.label :coloring %>
  <%= f.text_field :coloring %>
  <%= f.label :markings %>
  <%= f.text_field :markings %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :tag %>
  <%= f.text_field :tag %>

  <%= f.submit %>
<% end %>
